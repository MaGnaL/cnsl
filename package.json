{
  "name": "cnsl",
  "version": "2.0.0",
  "description": "Wrapper for console functionality. Wraps console methods and provides additional features.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "reinstall": "npm install && npm prune",
    "prepare": "npm run build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "pack": "npm pack",
    "format": "prettier --write \"lib/**/*.ts\"",
    "format:check": "prettier --check \"lib/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaGnaL/cnsl.git"
  },
  "author": {
    "name": "Manuel Lang",
    "email": "code@manuellang.dev",
    "url": "https://manuellang.dev/"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@magnal/prettier-config": "^1.0.2",
    "@types/lodash": "^4.14.141",
    "husky": "^4.0.10",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "lodash": "^4.17.15"
  }
}
